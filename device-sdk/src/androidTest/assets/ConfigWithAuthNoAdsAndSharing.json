{
  "data": {
    "type": "config",
    "links": {
      "self": "https://host.oddworks.io/v2/configs/"
    },
    "attributes": {
      "features": {
        "authentication": {
          "enabled": true
        },
        "ads": {
          "enabled": false
        },
        "metrics": {
          "appInit": {
            "action": "app:init",
            "enabled": true
          },
          "viewLoad": {
            "action": "view:load",
            "enabled": true
          },
          "videoPlay": {
            "action": "video:play",
            "enabled": true
          },
          "videoPlaying": {
            "action": "video:playing",
            "enabled": true,
            "interval": 30000
          },
          "videoStop": {
            "action": "video:stop",
            "enabled": true
          },
          "videoError": {
            "action": "video:error",
            "enabled": true
          }
        },
        "sharing": {
          "enabled": true,
          "text": "Watch the show live on mobile and TV connected devices!"
        }
      },
      "views": {
        "homepage": "custom-homepage",
        "menu": "custom-menu",
        "splash": "custom-splash"
      }
    }
  },
  "meta": {
    "device": "GOOGLE_ANDROID",
    "queryParams": {},
    "userEntitlements": []
  },
  "links": {
    "self": "https://host.oddworks.io/v2/config"
  }
}
